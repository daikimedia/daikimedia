{
  "extends": [
    "eslint:recommended", 
    "next", 
    "next/core-web-vitals", 
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "@next/next/no-img-element": "off",
    "no-unused-vars": "warn",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "react/no-unescaped-entities": "off",
    "jsx-a11y/alt-text": "warn"
  }
}